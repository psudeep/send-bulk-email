{
  "name": "bulk_email",
  "private": true,
  "version": "0.0.1",
  "description": "Code to Send bulk email",
  "keywords": [
    "email",
    "bulk",
    "sendgrid",
    "mailgun"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "send:sendgrid": "cross-env EMAIL_SERVICE=sendgrid node src/index.js",
    "send:mailgun": "cross-env EMAIL_SERVICE=mailgun node src/index.js",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon src/index.js",
    "docs:serve": "swagger-ui-express"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "json2csv": "^3.11.5",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.22.0",
    "moment": "^2.18.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.9.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
